package examples;

public class example1
{
	public static void main(String args[])
	{
		double[] myArray = {1.0, 2.3, 1.9};
		
		for(int i=0;i<myArray.length;i++)
		{
			System.out.println(myArray[i]+ "");
		}
		
		double total = 0;
		for(int i=0; i < myArray.length; i++)
		{
			total += myArray[i];
		}
		System.out.println("Total is " + total);
	}
}